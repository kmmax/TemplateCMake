#ifndef APP_VERSION_H
#define APP_VERSION_H

 /* The name of project */
#define APP_NAME "@APP_NAME@"

 /* The major version, (1, if %APP_VERSION is 1.2.3) */
#define APP_VERSION_MAJOR @APP_VERSION_MAJOR@

 /* The minor version (2, if %APP_VERSION is 1.2.3) */
#define APP_VERSION_MINOR @APP_VERSION_MINOR@

 /* The micro version (3, if %APP_VERSION is 1.2.3) */
#define APP_VERSION_BUILD @APP_VERSION_BUILD@

 /* The full version, like 1.2.3 */
#define APP_VERSION        @APP_VERSION@

 /* The full version, in string form (suited for string concatenation) */
#define APP_VERSION_STRING "@APP_VERSION@"

/* Shot description of project */
#define APP_DESCRIPTION 	"@APP_DESCRIPTION@"

/* URL of site homepage */
#define APP_HOMEPAGE_URL 	"@APP_HOMEPAGE_URL@"


#endif /* APP_VERSION_H */
